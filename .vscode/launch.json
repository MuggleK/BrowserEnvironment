{
    "version": "0.2.0",
    "configurations": [
    
        {
            "type": "node",
            "request": "launch",
            "name": "run",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\src\\index.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "without environment",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeExecutable": "node-inspect",
            "program": "${workspaceFolder}\\src\\index.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "without environment for production",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeExecutable": "node-inspect",
            "program": "${workspaceFolder}\\node_env.js"
        }
    ]
}